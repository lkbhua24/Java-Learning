package com.lkbhua;

public class demo {
    public static void main(String[] args) {
        /*  输出语句说明：
        * */
        // 输出语句
        // 两部分参数：
        // 第一部分参数：要输出的内容%s（占位符）
        // 第二部分参数：要填充的内容
        System.out.printf("hello,%s,%d,%f", "lkbhua", 18, 1.8);
        System.out.printf("帅哥%s", "lkbhua");
        // 注意：这样在输出时是不会换行的
        System.out.println(); // 换行
        System.out.printf("帅哥%s\n", "lkbhua");
    }


}
