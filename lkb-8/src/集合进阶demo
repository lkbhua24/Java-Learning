package com.lkbhua;


import java.util.Random;

public class 集合进阶demo {
    public static void main(String[] args) {
        int[] arr = { 1, 2, 3, 4, 5 };
        Random r = new Random();
        for (int i = 0; i < arr.length; i++){
            int randomindex = r.nextInt(arr.length);
            int temp = arr[i];
            arr[i] = arr[randomindex];
            arr[randomindex] = temp;
        }

    }
}
