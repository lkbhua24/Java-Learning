package com.lkbhua.interfaceDemo.demo1;

public class Test {
    public static void main(String[] args) {
        Frog frog = new Frog("青蛙王子", 2);
        System.out.println(frog.getName());
        System.out.println(frog.getAge());
        frog.eat();
        frog.swim();

        Rabbit rabbit = new Rabbit("小白", 1);
        System.out.println(rabbit.getName());
        System.out.println(rabbit.getAge());
        rabbit.eat();

        Dog dog = new Dog("哈士奇", 2);
        System.out.println(dog.getName());
        System.out.println(dog.getAge());
        dog.eat();
        dog.swim();
    }
}
