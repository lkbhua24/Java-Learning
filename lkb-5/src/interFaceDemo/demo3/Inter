package com.lkbhua.interfaceDemo.demo3;

public interface Inter {

    /* 接口中默认方法的格式：
    访问修饰符 返回值类型 方法名(参数列表) {方法体}
     public default void -----           */
    public default void show() {
        System.out.println("接口中的默认方法--- show");
    }


    public abstract void method();

    // public 可以省略
    // 但是default不可以省略
    // 如果实现了多个接口，多个接口中存在相同名字的默认方法，则必须重写该方法
    public default void show2() {
        System.out.println("接口中的默认方法--- show2");
    }


    /* 静态方法定义的格式：
          格式： public static 返回值类型 方法名(参数列表) {方法体}
          范例： public static void show3() {}
    * */

    public static void show3() {
        System.out.println("接口中的静态方法--- show3");
    }
}
