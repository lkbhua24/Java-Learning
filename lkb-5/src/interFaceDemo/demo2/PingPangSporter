package com.lkbhua.interfaceDemo.demo2;

public class PingPangSporter extends Sporter implements English{

    public PingPangSporter() {}

    public PingPangSporter(String name, int age) {
        super(name, age);
    }

    @Override
    public void speakEnglish() {
        System.out.println("PingPangSporter is speaking English");
    }

    @Override
    public void study() {
        System.out.println("PingPangSporter is studying");
    }
}
