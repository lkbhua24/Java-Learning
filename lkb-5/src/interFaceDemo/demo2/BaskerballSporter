package com.lkbhua.interfaceDemo.demo2;

public class BaskerballSporter extends Sporter implements English{
    public BaskerballSporter() {
    }

    public BaskerballSporter(String name, int age) {
        super(name, age);
    }

    @Override
    public void speakEnglish() {
        System.out.println("BaskerballSporter is speaking English");
    }

    @Override
    public void study() {
        System.out.println("BaskerballSporter is studying");
    }
}
