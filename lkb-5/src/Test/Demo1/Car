package com.lkbhua.Test.Demo1;

public class Car extends Animal{
    public Car(){}

    public Car(int age, String color){
        super(age, color);
    }

    @Override
    public void eat(String something){
        System.out.println(getAge()+"岁的"+getColor()+"颜色的猫咪眯着眼睛吃" + something);
    }

    public void catchMouse(){
        System.out.println("抓老鼠");
    }
}
