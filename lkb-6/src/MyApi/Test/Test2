package com.lkbhua.MyApi.Test;

public class Test2 {
    public static void main(String[] args) {
        // 求自幂数

        int count = 0;

        // 得到每一个三位数
        for(int i = 100; i < 1000; i++){
            // 得到每一位
            int ge = i % 10;
            int shi = i / 10 % 10;
            int bai = i / 100 % 10;

            // 判断
            int sum = (int) Math.pow(ge, 3) + (int) Math.pow(shi, 3) + (int) Math.pow(bai, 3);
            if(sum == i){
                count++;
                System.out.println(i);
            }
        }
        System.out.println("自幂数有：" + count + "个");
    }

}
